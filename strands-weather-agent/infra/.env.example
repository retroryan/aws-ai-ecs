# AWS Strands Weather Agent Configuration
# Copy this file to .env and update with your values

# Required: AWS Bedrock Model
# Use inference profiles (us. prefix) for cross-region redundancy
BEDROCK_MODEL_ID=us.anthropic.claude-3-5-sonnet-20241022-v2:0
# BEDROCK_MODEL_ID=us.anthropic.claude-3-5-haiku-20241022-v1:0
# BEDROCK_MODEL_ID=us.meta.llama3-1-70b-instruct-v1:0

# AWS Configuration
AWS_REGION=us-east-1
BEDROCK_REGION=us-east-1
BEDROCK_TEMPERATURE=0

# Stack Names (optional - defaults shown)
BASE_STACK_NAME=strands-weather-agent-base
SERVICES_STACK_NAME=strands-weather-agent-services
ENVIRONMENT_NAME=strands-weather-agent

# Logging
LOG_LEVEL=INFO

# Langfuse Telemetry (optional)
# Uncomment and fill in to enable observability
# LANGFUSE_PUBLIC_KEY=your-public-key
# LANGFUSE_SECRET_KEY=your-secret-key
# LANGFUSE_HOST=https://us.cloud.langfuse.com
# ENABLE_TELEMETRY=true
# TELEMETRY_USER_ID=default-user
# TELEMETRY_SESSION_ID=default-session
# TELEMETRY_TAGS=demo,aws-strands